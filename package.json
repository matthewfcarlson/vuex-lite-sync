{
  "name": "vuex-lite-sync",
  "version": "0.0.1",
  "description": "",
  "main": "dist/vuex-lite-sync.cjs.js",
  "module": "dist/vuex-lite-sync.es.js",
  "unpkg": "dist/vuex-lite-sync.min.js",
  "jsdelivr": "dist/vuex-lite-sync.min.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=7.8.0"
  },
  "scripts": {
    "test": "jest",
    "build": "bili",
    "codecov": "codecov",
    "pretty": "prettier --write 'src/**/*.js'",
    "preversion": "npm run test && npm run build",
    "version": "npm publish",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matthewfcarlson/vuex-lite-sync.git"
  },
  "babel": {
    "presets": "env"
  },
  "bili": {
    "format": [
      "umd",
      "umd-min",
      "cjs",
      "es"
    ],
    "moduleName": "Vuex",
    "global": {
      "vue": "Vue"
    },
    "buble": {
      "transforms": {
        "dangerousForOf": true
      }
    },
    "sizeLimit": {
      "es": "3KB",
      "cjs": "3KB",
      "umd": "3KB",
      "umd-min": "2KB"
    },
    "exports": "named"
  },
  "prettier": {
    "useTabs": false,
    "printWidth": 80,
    "tabWidth": 2,
    "singleQuote": false,
    "trailingComma": "none",
    "jsxBracketSameLine": false,
    "semi": true
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.{js,es6}"
    ],
    "coverageDirectory": ".coverage"
  },
  "keywords": [
    "state-manager",
    "vue"
  ],
  "author": "Matthew Carlson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matthewfcarlson/vuex-lite-sync/issues"
  },
  "devDependencies": {
    "@types/node": "^13.9.2",
    "@types/vue": "^2.0.0",
    "bili": "^4.8.1",
    "codecov": "^3.0.0",
    "jest": "^25.1.0",
    "prettier": "^1.10.2",
    "vue": "^2.5.13"
  },
  "peerDependencies": {
    "vue": "^2.5.13"
  },
  "homepage": "https://github.com/matthewfcarlson/vuex-lite-sync#readme"
}

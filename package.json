{
  "name": "vuex-lite-sync",
  "version": "0.0.1",
  "description": "",
  "main": "dist/vuex-lite-sync.umd.js",
  "module": "dist/vuex-lite-sync.es.js",
  "unpkg": "dist/vuex-lite-sync.min.js",
  "jsdelivr": "dist/vuex-lite-sync.min.js",
  "typings": "dist/types/vuex-lite-sync.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=7.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matthewfcarlson/vuex-lite-sync.git"
  },
  "keywords": [
    "state-manager",
    "vue"
  ],
  "author": "Matthew Carlson",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/matthewfcarlson/vuex-lite-sync/issues"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'tests/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.0",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.0.1",
    "jest": "^25.1.0",
    "jest-config": "^25.1.0",
    "lint-staged": "^10.0.0",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.19.0",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^2.1.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "semantic-release": "^17.0.0",
    "shelljs": "^0.8.3",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.7.0",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.17.1",
    "@types/vue": "^2.0.0",
    "typescript": "^3.8.3",
    "vue": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "homepage": "https://github.com/matthewfcarlson/vuex-lite-sync#readme"
}
